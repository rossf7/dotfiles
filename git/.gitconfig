[alias]
  bd = branch -d   
  ci = commit
  co = checkout
  cob = checkout -b
  com = checkout master
  df = diff
  f = fetch -p    
  fo= fetch origin
  p = push
  plom = pull origin master
  st = status

[core]
    excludesfile = ~/.gitignore_global

[credential]
	helper = osxkeychain

[github]
	user = rossf7

[user]
	name = Ross Fairbanks
	email = ross@rossfairbanks.com
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
